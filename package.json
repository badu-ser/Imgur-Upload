  "description": "A library for upload images to imgur, no dependencies",
  "type": "module",
  "main": "dist/imgur.min.js",
  "module": "dist/imgur.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "umd": "./dist/imgur.min.js",
