    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.7.7",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
